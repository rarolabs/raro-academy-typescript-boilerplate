{
  "name": "api-bolao",
  "version": "0.0.1",
  "description": "Projeto desenvolvido no treinamento de NODE JS da raro academy",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "dev": "ts-node src/app.ts",
    "test": "jest",
    "lint:fix": "npm run lint -- --fix",
    "test:watch": "jest --watchAll ",
    "test:coverage": "jest --coverage ",
    "typeorm": "cross-env NODE_ENV=development ts-node ./node_modules/.bin/typeorm",
    "typeorm:win": "ts-node -r tsconfig-paths/register node_modules\\typeorm\\cli.js",
    "migration:generate:win": "npm run typeorm:win migration:generate -- -n"
  },
  "author": "pauloFernandes",
  "license": "ISC",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^8.2.0",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.0.1",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^16.10.3",
    "@types/node-fetch": "^3.0.3",
    "faker": "^5.5.3",
    "jest": "^27.2.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "axios": "^0.22.0",
    "dotenv": "^8.6.0",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.18.1",
    "node-fetch": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.38"
  }
}
